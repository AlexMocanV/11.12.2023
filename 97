#include <iostream>

using namespace std;
/*
a) 3 10 24
    
b)  1 11 111
*/
int main()
{
    int i,x,j,s;
    for(i=1;i<=3;i++)
    {
        cin>>x;
        s=0;
        for(j=1;j<=i;j++)
        {
            s=s+x%10;
        }
        cout<<s;
    }
    
    
    return 0;
}
#include <iostream>

using namespace std;
/*
a) 3 10 24
    
b)  1 11 111
*/
int main()
{
    int i,x,j,s;
    cin>>x>>y>>z;
    cout<<x%10<<" "<<y%10+(y/10)%10<<" "<<z%10+(z/10)%10+(z/100)%10;
    
    
    return 0;
}
